{
  "name": "internal-dashboard-backend",
  "version": "1.0.0",
  "description": "Payload CMS Backend for Internal Tool Dashboard",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build": "tsc",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node src/seed/index.ts"
  },
  "dependencies": {
    "payload": "^2.0.0",
    "@payloadcms/db-mongodb": "^1.0.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "express": "^4.18.3",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.0",
    "@types/cors": "^2.8.17",
    "typescript": "^5.4.0",
    "ts-node": "^10.9.2",
    "nodemon": "^3.1.0",
    "cross-env": "^7.0.3"
  }
}
